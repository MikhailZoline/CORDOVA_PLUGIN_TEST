<?xml version='1.0' encoding='utf-8'?>
<plugin id="com-mikhailcreate-plugins-test" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>coolMethod</name>
    <js-module name="coolMethod" src="www/coolMethod.js">
        <clobbers target="coolMethod" />
    </js-module>
<platform name="ios">
  <config-file target="config.xml" parent="/*">
    <feature name="coolMethod">
      <param name="ios-package" value="coolMethod" />
    </feature>
  </config-file>
  <source-file src="src/ios/coolMethod.swift" />
  <source-file src="src/ios/CollisionParticle.swift" />
  <source-file src="src/ios/Math.swift" />
  <source-file src="src/ios/ScreenSaverController.swift" />
</platform>
</plugin>
